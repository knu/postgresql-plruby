   p point, dp float, d0 float, c circle, w float, h float
);
create or replace function box_val(box[]) returns setof pl_box as '
   p = Point.new(6,4)
   args[0].each do |b| 
      p0 = b.center
      yield [p0, Geometry::distance(p, b),Geometry::distance(p, p0),
             b.to_circle, b.height, b.width]
   end
' language 'plruby';
